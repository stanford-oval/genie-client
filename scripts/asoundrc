pcm.g_voice {
  type plug
  slave {
    pcm "playroute"
    channels 2 
    format S16_LE
    rate 48000
  }
}

pcm.g_hd {
  type plug
  slave {
    pcm {
      type softvol
      slave.pcm "playroute"
      control {
        name "hd"
        card 0
      }
    }
    channels 2
    format S16_LE
    rate 48000
  }
}

pcm.g_alert {
  type plug
  slave.pcm "playroute"
}

pcm.playroute {
  type plug
  slave.pcm plugequal;
}
